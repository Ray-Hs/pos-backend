model supply {
    id Int @id @default(autoincrement())

    invoiceNO     String?
    companyId     Int
    company       CompanyInfo   @relation(fields: [companyId], references: [id])
    paymentMethod PaymentMethod @default(CASH)
    barcode       String?
    name          String
    packageQty    Int
    itemQty       Int
    packagePrice  Int
    itemPrice     Int
    itemSellPrice Float
    totalItems    Int?
    totalPrice    Float?
    store         String

    expiryDate DateTime?
    createdAt  DateTime  @default(now())
    updatedAt  DateTime  @updatedAt
}
